users:
  - name: test
    sudo: ALL=(ALL) NOPASSWD:ALL
    groups: users, admin
    home: /home/test
    shell: /bin/bash
    ssh-authorized-keys:
      - ssh-rsa  test #CHANGE_ME
ssh_pwauth: True
disable_root: false
chpasswd:
  list: |
     test:linux
  expire: False
package_update: true
packages:
    - qemu-guest-agent
growpart:
  mode: auto
  devices: ['/']

